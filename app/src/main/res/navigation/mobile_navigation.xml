<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.publishinghousekotlin.ui.gallery.GalleryFragment"
        android:label="@string/menu_bookings"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/typeProductsScreen"
        android:name="com.example.publishinghousekotlin.ui.typeProduct.TypeProductFragment"
        android:label="@string/menu_typeProducts"
        tools:layout="@layout/fragment_general" />

    <fragment
        android:id="@+id/materialsScreen"
        android:name="com.example.publishinghousekotlin.ui.material.MaterialFragment"
        android:label="@string/menu_materials"
        tools:layout="@layout/fragment_slideshow" />
    <fragment
        android:id="@+id/printingHousesScreen"
        android:name="com.example.publishinghousekotlin.ui.printingHouse.PrintingHouseFragment"
        android:label="@string/menu_printingHouses" />
    <fragment
        android:id="@+id/employeesScreen"
        android:name="com.example.publishinghousekotlin.ui.employee.EmployeeFragment"
        android:label="@string/menu_employees" />
</navigation>